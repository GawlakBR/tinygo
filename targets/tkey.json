{
    "inherits": ["riscv32"],
    "build-tags": ["tkey"],
    "features": "+c,+zmmul",
    "cflags": [
        "-mcmodel=medany",
        "-march=rv32iczmmul"
    ],
    "linkerscript": "targets/tkey.ld",
    "scheduler": "none",
    "default-stack-size": 2048,
    "gc": "leaking",
    "linker": "ld.lld",
    "rtlib": "compiler-rt",
    "libc": "picolibc",
    "flash-command": "tkey-runapp {bin}",
    "serial": "uart"
}
