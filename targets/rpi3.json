{
	"build-tags": ["rpi3","baremetal"],
	"goarch":"arm64",
	"llvm-target":"aarch64-none-linux-elf",
	"gc": "leaking",
	"compiler":"clang",
	"linker":"ld.lld",
	"rtlib": "compiler-rt",
	"cflags": [
		"--target=aarch64-elf",
		"-Oz",
		"-fshort-enums",
		"-nostdlibinc",
		"-Wno-macro-redefined",
		"-fno-exceptions",
		"-fno-unwind-tables",
		"-ffunction-sections",
		"-fdata-sections"
	],
	"ldflags": [
		"--gc-sections",
		 "-T", "targets/rpi3.ld"
	],
	"extra-files": [
		"src/device/rpi3/start.S",
		"src/device/rpi3/entry.S"
	],
	"gdb": ""
}
